#pragma config(Sensor, dgtl1,  rightencoder,   sensorQuadEncoder)
#pragma config(Sensor, dgtl3,  leftencoder,    sensorQuadEncoder)
#pragma config(Motor,  port1,           leftmotor,     tmotorVex393_HBridge, openLoop, driveLeft)
#pragma config(Motor,  port9,           clawmotor,     tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port10,          rightmotor,    tmotorVex393_HBridge, openLoop, reversed, driveRight)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()

{

while(1==1)
	{

	if (vexRT[Btn5U]==1)
		{
		motor[clawmotor]=34;
		}
	else if (vexRT[Btn6U]==1)
		{
		motor[clawmotor]=-34;
		}
	else
	  {
		motor[clawmotor]=0;
	  }
  motor[rightmotor]= vexRT[Ch3]/3;
	motor[leftmotor]= vexRT[Ch2]/3;

	}

}
